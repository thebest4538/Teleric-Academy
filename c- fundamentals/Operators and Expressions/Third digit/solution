using System;


namespace Devideby5and7
{
    class Program
    {
        static void Main()
        {
            int n = int.Parse(Console.ReadLine());
            n = (n / 100) % 10;
            if ( n== 7 )
            {
                Console.WriteLine("true");
            }
            else
            {
                Console.WriteLine("false" + " " + n);
            }
        }
    }
}
